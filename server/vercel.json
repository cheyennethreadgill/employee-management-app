{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://kubermanagement.netlify.app" },
        { "key": "Access-Control-Allow-Headers", "value": "Accept, Content-Type" }
      ]
    }
  ]
}
